<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="superVBox" minWidth="-Infinity" onDragDetected="#dragDetected" onDragDone="#dragDone" prefWidth="240.0" spacing="1.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.uncg.save.controllers.DataPaneController">
    <children>
        <Label fx:id="dataTextLabel" alignment="TOP_LEFT" graphicTextGap="0.0" maxWidth="-Infinity" prefWidth="335.0" text="label" wrapText="true">
            <VBox.margin>
                <Insets />
            </VBox.margin>
            <font>
                <Font size="18.0" />
            </font>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
        </Label>
        <TitledPane fx:id="metadataTitledPane" animated="false" expanded="false" onMouseReleased="#modifyMetaPane" prefWidth="240.0" text="Metadata">
            <content>
                <VBox fx:id="metadataVBox" prefWidth="238.0">
                    <children>
                        <HBox alignment="CENTER_LEFT" spacing="25.0">
                            <children>
                                <Label minWidth="-Infinity" prefWidth="90.0" text="Source:" />
                                <Label fx:id="dataSourceLabel" prefWidth="200.0" text="dataSource" wrapText="true" />
                            </children>
                        </HBox>
                        <Separator prefHeight="10.0" prefWidth="200.0" />
                        <HBox alignment="CENTER_LEFT" spacing="25.0">
                            <children>
                                <Label minWidth="-Infinity" prefWidth="90.0" text="Date:" />
                                <Label fx:id="dataDateLabel" prefWidth="200.0" text="dataDate" wrapText="true" />
                            </children>
                        </HBox>
                        <Separator prefHeight="10.0" prefWidth="200.0" />
                        <HBox alignment="CENTER_LEFT" spacing="25.0">
                            <children>
                                <Label minWidth="-Infinity" prefWidth="90.0" text="Genre:" />
                                <Label fx:id="dataGenreLabel" prefWidth="200.0" text="dataGenre" wrapText="true" />
                            </children>
                        </HBox>
                        <Separator prefHeight="10.0" prefWidth="200.0" />
                        <HBox alignment="CENTER_LEFT" spacing="25.0">
                            <children>
                                <Label minWidth="-Infinity" prefWidth="90.0" text="Reliability:" />
                                <Label fx:id="dataReliabilityLabel" prefWidth="200.0" text="dataRel" wrapText="true" />
                            </children>
                        </HBox>
                        <Separator prefHeight="10.0" prefWidth="200.0" />
                        <HBox alignment="CENTER_LEFT" spacing="25.0">
                            <children>
                                <Label minWidth="-Infinity" prefWidth="90.0" text="Likelihood:" />
                                <Label fx:id="dataLikelihoodLabel" prefWidth="200.0" text="dataLike" wrapText="true" />
                            </children>
                        </HBox>
                        <Separator prefHeight="10.0" prefWidth="200.0" />
                        <HBox alignment="CENTER_LEFT" spacing="25.0">
                            <children>
                                <Label minWidth="-Infinity" prefWidth="90.0" text="Comment:" />
                                <Label fx:id="dataCommentLabel" prefWidth="200.0" text="dataComment" wrapText="true" />
                            </children>
                        </HBox>
                    </children>
                </VBox>
            </content>
            <font>
                <Font size="18.0" />
            </font>
        </TitledPane>
    </children>
</VBox>
